
@{
    ViewData["Title"] = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="card-header border-0">
    <h3 class="mb-0 ">Deneyim Ekleme Sayfası</h3>

</div>
<form class="form-group" method="post">
    <label class="mt-2"> Deneyim Başlığı</label>
    <input type="text" name="Name" class="form-control"/>
    <label  > Deneyim Tarihi</label>
    <input type="text" name="Date" class="form-control"/>
    <label> Deneyim İçeriği</label>
    <input type="text" name="Description" class="form-control" />
    <button class="btn btn-primary mt-2" type="submit">Kaydet</button> 

</form>
